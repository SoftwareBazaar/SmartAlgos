{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "REACT_APP_API_URL": "https://smart-algos.vercel.app/api",
    "REACT_APP_WS_URL": "https://smart-algos.vercel.app",
    "REACT_APP_VISME_EMBED_ENABLED": "false",
    "NODE_ENV": "production"
  }
}